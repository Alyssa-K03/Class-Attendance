--students table

  CREATE TABLE "STUDENTS" 
   (	"STUDENT_NUMBER" CHAR(7 CHAR) NOT NULL ENABLE, 
	"STUDENT_FIRST_NAME" VARCHAR2(50 CHAR) NOT NULL ENABLE, 
	"STUDENT_LAST_NAME" VARCHAR2(50 CHAR) NOT NULL ENABLE, 
